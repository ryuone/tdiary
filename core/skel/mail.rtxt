From: <%= name %> <<%= mail %>>
To: <%= @author_mail %>
Date: <%= date %>
Message-Id: <%= message_id %>
Subject: <%= @mail_header %>:<%= @date.strftime( '%Y-%m-%d' ) %>-<%= serial %> 
	<%= to_mime( @name.to_jis ) %>
MIME-Version: 1.0
Content-Type: text/plain; charset="iso-2022-jp"
Content-Transfer-Encoding: 7bit
Errors-To: <%= @author_mail %>
X-Mailer: tDiary <%= TDIARY_VERSION %>
X-URL: http://www.tdiary.net/

<%= body %>

